n = int(input())

first_range = 0
second_range = 0
third_range = 0
fourth_range = 0
fifth_range = 0

for i in range(0, n):
    number = int(input())
    if number < 200:
        first_range += 1
    elif 200 <= number <= 399:
        second_range += 1
    elif 400 <= number <= 599:
        third_range += 1
    elif 600 <= number <= 799:
        fourth_range += 1
    elif 800 <= number:
        fifth_range += 1

total_amount_of_numbers = n

first_range_percentage = first_range / total_amount_of_numbers * 100
second_range_percentage = second_range / total_amount_of_numbers * 100
third_range_percentage = third_range / total_amount_of_numbers * 100
fourth_range_percentage = fourth_range / total_amount_of_numbers * 100
fifth_range_percentage = fifth_range / total_amount_of_numbers * 100

print(f'{first_range_percentage:.2f}%')
print(f'{second_range_percentage:.2f}%')
print(f'{third_range_percentage:.2f}%')
print(f'{fourth_range_percentage:.2f}%')
print(f'{fifth_range_percentage:.2f}%')
