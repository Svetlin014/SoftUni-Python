hours = int(input())
minutes = int(input())

total_time = hours * 60 + minutes + 15

final_hours = total_time // 60
final_minutes = total_time % 60

if final_hours == 24:
    final_hours = 0

if final_minutes < 10:
    print(f'{final_hours}:0{final_minutes}')
else:
    print(f'{final_hours}:{final_minutes}')
